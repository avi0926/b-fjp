import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int val = 1;
        
        for(int i=1; i<=n; i++){
            for(int j=1; j<=i; j++){
                System.out.print(val+"\t");
                val++;
            }
            System.out.println();
        }
    }
}
//////////////////////////////////////////////////////////////////////
0 1 1 2 3 5 8 13 
import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int n = 8;
        
        int a = 0;
        int b = 1;
        for(int i=1; i<=n; i++){
            System.out.print(a+" ");
            int c = a+b;
            a = b;
            b = c;
        }
    }
}
